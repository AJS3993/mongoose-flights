

<%- include('../partials/header') %>

  <h2>Flight List</h2>
  <table id='list'>
<thead>
    <th>Airport</th>
    <th>Airline</th>
    <th>Flight Number</th>
    <th>Departure</th>
</thead>
<% flight.forEach(function(flight){ %>
    <tr>
        <td><%= flight.airport %></td>
        <td><%= flight.airline %></td>
        <td><%= flight.flightNo %></td>
        <td><%= flight.departure %></td>
        <td><a href="/flights/<%= flight._id %>"  id="details">DETAILS</a></td>
    </tr>
<% }) %>
  </table>

  <%- include('../partials/footer') %>


